{
  "name": "monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Rust-like standard library without undefined behavior",
  "scripts": {
    "test": "pnpm run -r --stream --parallel --color test",
    "build": "pnpm run -r --stream --parallel --color build:package",
    "lint": "eslint packages/**/src --ext .ts --cache",
    "lint:fix": "pnpm lint --fix",
    "dx": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.2",
    "@tsconfig/node18": "18.2.0",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.5",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "esbuild": "0.18.17",
    "eslint": "8.45.0",
    "eslint-plugin-license-header": "0.6.0",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6",
    "zx": "7.2.3"
  },
  "keywords": [
    "rust",
    "rslike",
    "undefiend behavior",
    "std",
    "standard"
  ],
  "author": "Vitali Haradkou <vitalicset@yandex.ru>",
  "license": "MIT",
  "dependencies": {
    "typedoc": "0.24.8"
  }
