{
  "name": "@rslike/std",
  "version": "1.0.0",
  "description": "JavaScript Standard library without udndefined behavior!",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build:base": "esbuild --bundle --tree-shaking=true",
    "build:package:types": "tsc",
    "build:package:globals:cjs": "pnpm build:base ./src/globals.ts --external:./ --outfile=dist/globals.cjs --format=cjs",
    "build:package:globals:esm": "pnpm build:base ./src/globals.ts --external:./ --outfile=dist/globals.esm.cjs --format=esm",
    "build:package:cjs": "pnpm build:base ./src/index.ts --outfile=dist/index.cjs --format=cjs",
    "build:package:esm": "pnpm build:base ./src/index.ts --outfile=dist/index.esm.js --format=esm",
    "build:package": "pnpm run '/^build:package:.*/'",
    "docs": "pnpm typedoc src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    },
    "./globals": {
      "types": "./dist/globals.d.ts",
      "import": "./dist/globals.esm.js",
      "require": "./dist/globals.cjs"
    }
  },
  "keywords": [
    "std",
    "standard",
    "rust like",
    "rust",
    "option",
    "result"
  ],
  "author": "Vitali Haradkou",
  "license": "ISC"
}