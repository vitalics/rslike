{
  "name": "@rslike/dbg",
  "version": "3.1.1",
  "description": "Never use console.log to debug again!",
  "private": false,
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "docs": "pnpm typedoc src/index.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "keywords": [
    "std",
    "standard",
    "undefined behavior",
    "debug",
    "dbg",
    "inspection"
  ],
  "author": "Vitali Haradkou",
  "license": "MIT",
  "homepage": "https://github.com/vitalics/rslike/blob/main/packages/dbg/README.md",
  "bugs": {
    "url": "https://github.com/vitalics/rslike/issues",
    "email": "vitalicset@yandex.ru"
  }
}
